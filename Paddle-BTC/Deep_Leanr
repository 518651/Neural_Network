#  读取数据

def load_data():
    datafile = pd.read_excel('BTC-COIN-DATA.xlsx', sheet_name=0, index_col=0)
    print(datafile)

    feature_names = ['Date', 'Low', 'High', 'Open', 'Close', 'Volume']
    feature_num = len(feature_names)

    

load_data()



